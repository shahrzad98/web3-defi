  "scripts": {
    "start": "npm run  start:test",
    "start:test": "cat .env.develop > .env.local && cat src/config/testnet.json > src/config/config.json && react-scripts start",
    "start:hotjar": "export HTTPS=true&&SSL_CRT_FILE=cert.pem&&SSL_KEY_FILE=key.pem npm run  start:test",
    "start:main": "cat .env.production > .env.local && cat src/config/mainnet.json > src/config/config.json && react-scripts start",
    "build": "npm run build:test",
    "build:test": "cat .env.develop > .env.local && cat src/config/testnet.json > src/config/config.json && react-scripts build",
    "build:main": "cat .env.production > .env.local && cat src/config/mainnet.json > src/config/config.json && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "npm run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint src",
    "lint:fix": "npm run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@types/lodash": "4.14.170",
    "@types/moment": "2.13.0",
    "@types/react-gtm-module": "2.0.1",
    "ts-standard": "10.0.0"
  }
